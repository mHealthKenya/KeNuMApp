{
	"expo": {
		"name": "KeNuM",
		"slug": "KeNuMApp",
		"version": "1.0.3",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"config": {
				"usesNonExemptEncryption": false
			},
			"supportsTablet": true,
			"bundleIdentifier": "com.ikode.KeNuMApp"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.ikode.KeNuMApp",
			"permissions": ["android.permission.RECORD_AUDIO"]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you upload certificates and CPD evidence."
				}
			],
			[
				"expo-document-picker",
				{
					"iCloudContainerEnvironment": "Production"
				}
			],
			[
				"expo-updates",
				{
					"username": "iKode"
				}
			],
			[
				"expo-font",
				{
					"fonts": [
						"./assets/fonts/san.otf",
						"./assets/fonts/sanbold.otf",
						"./assets/fonts/italic.otf",
						"./assets/fonts/SpaceMono-Regular.ttf",
						"./assets/fonts/SF-Pro.ttf",
						"./assets/fonts/RobotoMono-Bold.ttf",
						"./assets/fonts/RobotoMono-Regular.ttf",
						"./assets/fonts/RobotoMono-ThinItalic.ttf",
						"./assets/fonts/RobotoMono-Italic.ttf",
						"./assets/fonts/Roboto-Regular.ttf",
						"./assets/fonts/Roboto-Bold.ttf",
						"./assets/fonts/Roboto-Italic.ttf",
						"./assets/fonts/SourceCodePro-Regular.ttf",
						"./assets/fonts/SourceCodePro-Italic.ttf",
						"./assets/fonts/SourceCodePro-Bold.ttf"
					]
				}
			],
			"expo-secure-store"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "f203723c-5920-4a31-90b4-974d06dbedb9"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/f203723c-5920-4a31-90b4-974d06dbedb9"
		}
	}
}
