{
	"cli": {
		"version": ">= 5.6.0",
		"appVersionSource": "remote"
	},
	"build": {
		"preview": {
			"android": {
				"buildType": "apk"
			},
			"channel": "preview"
		},
		"preview2": {
			"android": {
				"gradleCommand": ":app:assembleRelease"
			},
			"channel": "preview2"
		},
		"preview3": {
			"developmentClient": true,
			"channel": "preview3"
		},

		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"production": {
			"channel": "production",
			"autoIncrement": true
		}
	}
}
